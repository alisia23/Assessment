import numpy as np

def function():
    c = 0
    try:
        while c != 4:
            a = np.random.randint(1, 10)
            b = np.random.randint(1, 10)
            c = a*b
            if c == 4:
                print(f"The final values are a: {a}, b: {b}")
                break
            else:
                print(f"The value of a is: {a} and the value of c is: {c}")
            
    except Exception as e:
        print(f"Error: {e}")
